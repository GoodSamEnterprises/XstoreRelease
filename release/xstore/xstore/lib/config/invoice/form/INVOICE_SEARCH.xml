              <ColorGroup fgColor="#9f2d2b" />
              <Font fontStyle="BOLD"/>
            </FormViewCell>
            <FormViewCell layoutLocation="4, 12, 1, 1, F, F" type="FormTextField" resource="invoiceDateEnd">
              <TabOrder dtype="Integer">6</TabOrder>
              <DataField ref="Date"/>
