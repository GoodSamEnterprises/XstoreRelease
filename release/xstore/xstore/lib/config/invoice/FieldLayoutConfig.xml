<?xml version="1.0" encoding="UTF-8"?>
<FieldLayoutSet xmlns="http://micros.com/xstore/config/form/fieldlayout" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FieldLayoutConfig.xsd">

  <FieldLayout name="taxFree_customerHeader" type="ListLayout" >
    <Row>
      <Column start="0" width="100" fieldRef="taxFree_customerTitleLabel" verticalAlignment="Center" fontRef="fullScreenFormHeaderLabel" />
    </Row>
  </FieldLayout>

  <FieldLayout name="taxFree_customerInfoSummary" type="ListLayout">
    <Row>
      <Column start="0" width="100" fieldRef="taxFree_customerGroupLabel" fontRef="formContentHeader" verticalAlignment="Center" />
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_firstNameLabel" horizontalAlignment="Right" />
      <Column start="15" width="35" fieldRef="taxFree_firstName" />
      <Column start="50" width="14" fieldRef="taxFree_lastNameLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_lastName" />
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_addressLabel" horizontalAlignment="Right"/>
      <Column start="15" width="85" fieldRef="taxFree_address" />
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_cityLabel" horizontalAlignment="Right" />
      <Column start="15" width="35" fieldRef="taxFree_city" />
      <Column start="50" width="14" fieldRef="taxFree_stateLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_state" />
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_postalCodeLabel" horizontalAlignment="Right" />
      <Column start="15" width="20" fieldRef="taxFree_postalCode" />
      <Column start="35" width="29" fieldRef="taxFree_countryLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_country" />
    </Row>
    <Row>
      <Column start="0" width="100" fieldRef="taxFree_emptyLabel" />
    </Row>
    <Row>
      <Column start="0" width="100" fieldRef="taxFree_invoiceGroupLabel" fontRef="formContentHeader" verticalAlignment="Center" />
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_identityDocTypeLabel" horizontalAlignment="Right" />
      <Column start="15" width="28" fieldRef="taxFree_identityDocType" />
      <Column start="43" width="21" fieldRef="taxFree_identityDocNumLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_identityDocNum" />      
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_identityDocCountryLabel" horizontalAlignment="Right" />
      <Column start="15" width="28" fieldRef="taxFree_identityDocCountry" />
      <Column start="43" width="21" fieldRef="taxFree_taxRegistrationNumberLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_taxRegistrationNumber" />           
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_mobileNumberLabel" horizontalAlignment="Right" />
      <Column start="15" width="28" fieldRef="taxFree_mobileNumber" />
      <Column start="43" width="21" fieldRef="taxFree_birthdayLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_birthday" /> 
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_finalDestinationLabel" horizontalAlignment="Right" />
      <Column start="15" width="20" fieldRef="taxFree_finalDestination" />
      <Column start="35" width="29" fieldRef="taxFree_emailLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_email" />
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_arrivalDateLabel" horizontalAlignment="Right" />
      <Column start="15" width="20" fieldRef="taxFree_arrival" />
      <Column start="35" width="29" fieldRef="taxFree_departureDateLabel" horizontalAlignment="Right" />
      <Column start="65" width="20" fieldRef="taxFree_departure" />
    </Row>
    <Row>
      <Column start="0" width="14" fieldRef="taxFree_serviceLabel" horizontalAlignment="Right" visibilityGroup="taxFreeServiceVisibility" />
      <Column start="15" width="28" fieldRef="taxFree_service" refreshOnChange="true" visibilityGroup="taxFreeServiceVisibility" />
      <Column start="43" width="21" fieldRef="taxFree_refundMehodLabel" horizontalAlignment="Right" />
      <Column start="65" width="35" fieldRef="taxFree_refundMethod" />
    </Row>
  </FieldLayout>

  <FieldLayout name="taxFree_identityCheck" type="SimpleLayout">
    <Row>
      <Column fieldRef="taxFree_firstNameLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_firstName" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_lastNameLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_lastName" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_passportNumLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_passport" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_countryLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_countryOfResidence" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_emptyLabel" />
      <Column />
    </Row>
    <Row>
      <Column fieldRef="taxFree_shopAssistantLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_shopAssistant" />
    </Row>
  </FieldLayout>

  <FieldLayout name="taxFree_providerCard" type="SimpleLayout">
    <Row>
      <Column fill="true" fieldRef="taxFree_providerCardLabel" fontRef="formContentHeader" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_providerCardNumLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_providerCardNumber" />
    </Row>
    <Row>
      <Column fill="true" fieldRef="taxFree_emptyLabel" />
    </Row>
    <Row>
      <Column fill="true" fieldRef="taxFree_passportLabel" fontRef="formContentHeader" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_passportNumLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_passport" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_passportCountryLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_passportCountry" />
    </Row>
    <Row>
      <Column fill="true" fieldRef="taxFree_emptyLabel" visibilityGroup="taxFreePhoneSearchVisibility" />
    </Row>
    <Row>
      <Column fill="true" fieldRef="taxFree_mobilePhoneLabel" fontRef="formContentHeader" verticalAlignment="Center" visibilityGroup="taxFreePhoneSearchVisibility" />
    </Row>    
    <Row>
      <Column fieldRef="taxFree_phoneNumberLabel" horizontalAlignment="Right" visibilityGroup="taxFreePhoneSearchVisibility" />
      <Column fieldRef="taxFree_mobPhoneNumber" visibilityGroup="taxFreePhoneSearchVisibility" />
    </Row>
  </FieldLayout>

  <FieldLayout name="taxFree_refundCardToken" type="SimpleLayout">
    <Row>
      <Column fieldRef="taxFree_cardTokenLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_cardToken" />
    </Row>
    <Row>
      <Column fieldRef="taxFree_emptyLabel" />
      <Column />
    </Row>
    <Row>
      <Column fieldRef="taxFree_refundCardTypeLabel" horizontalAlignment="Right" visibilityGroup="taxFreeRefundCardTypeVisibility" />
      <Column fieldRef="taxFree_refundCardType" visibilityGroup="taxFreeRefundCardTypeVisibility" />
    </Row>
  </FieldLayout>

  <FieldLayout name="taxFree_refundAlipayNumber" type="SimpleLayout">
    <Row>
      <Column fieldRef="taxFree_refundPhoneNumberLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_alipayNumber" />
    </Row>
  </FieldLayout>

  <FieldLayout name="taxFree_refundEtihadNumber" type="SimpleLayout">
    <Row>
      <Column fieldRef="taxFree_etihadNumberLabel" horizontalAlignment="Right" />
      <Column fieldRef="taxFree_etihadNumber" />
    </Row>
  </FieldLayout>

  <FieldLayout name="taxFree_refundEmailPhone" type="ListLayout">
    <Row>
      <Column start="0" width="100" fieldRef="taxFree_emailMobileInstructionsLabel" fontRef="formContentHeader" />
    </Row>
    <Row>
      <Column start="0" width="100" fieldRef="blueLineSeparator" verticalAlignment="Center" />
    </Row>
    <Row>
      <Column start="0" width="20" fieldRef="taxFree_refundEmailAddressLabel" horizontalAlignment="Right" />
      <Column start="21" width="79" fieldRef="taxFree_email" />
    </Row>
    <Row>
      <Column start="0" width="100" fieldRef="taxFree_emptyLabel" visibilityGroup="taxFreePhoneSearchVisibility" />
    </Row>
    <Row>
      <Column start="0" width="20" fieldRef="taxFree_phoneNumberLabel" horizontalAlignment="Right" />
      <Column start="21" width="20" fieldRef="taxFree_mobPhonePrefix" />
      <Column start="45" width="55" fieldRef="taxFree_mobPhoneNumber" />
    </Row>
  </FieldLayout>


</FieldLayoutSet>