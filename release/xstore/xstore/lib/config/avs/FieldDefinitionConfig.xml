<?xml version="1.0" encoding="UTF-8"?>
<FieldSet xmlns="http://micros.com/xstore/config/form/field" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormFieldConfig.xsd">
  
  <Field name="addressLineLabel" type="Label" resource="addressLineLabel"/>
  
  <Field name="addressLine" type="Text" resource="addressLine"  />
  
  <Field name="addressGroupLabel" type="Label" resource="addressGroupLabel"/>
  
  <Field name="addressGroups" type="List" resource="addressGroupListModel">
    <Parameter name="CellType" value="ADDRESS_GROUPS" />
    <!-- Possible values [SINGLE_SELECTION, MULTIPLE_SELECTION, NO_SELECTION] -->
    <Parameter name="ListSelectionMode" value="SINGLE_SELECTION" />  
  </Field>
  
  <!-- mobile only -->
  <Field name="selectedAddressLabel" type="Label" resource="selectedAddressLabel"/>
  
</FieldSet>