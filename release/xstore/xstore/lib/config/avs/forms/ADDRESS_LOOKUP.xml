<FormSet xmlns="http://micros.com/xstore/config/form" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormConfig.xsd">
  <Form name="ADDRESS_LOOKUP" type="POPUP_VIEW_PANEL">
    <Page name="addressLookup">
      <Panel startX="0" width="100" startY="0" height="100" fieldLayoutRef="addressLookupLayout" />      
    </Page>
    <ActionGroup name="DEFAULT">
      <Action dataKey="ADDRESS_LOOKUP_CANCEL" text="_cancel" keyStroke="Escape" />
	    <Action dataKey="ADDRESS_LOOKUP_CLEAR_HISTORY"   text="_addressLookupStartNewSearch" keyStroke="F2" />
