	    <Action dataKey="REFINE_WITH_ADDRESS_LIST"  text="_refineAddress"  keywords="actionForSelectedListItem" keyStroke="F8" />
    </ActionGroup>
    <ActionGroup name="REFINE_WITHOUT_ADDRESS_LIST">
      <Action dataKey="ADDRESS_LOOKUP_CANCEL" text="_cancel" keyStroke="Escape" />
      <Action dataKey="ADDRESS_LOOKUP_CLEAR_HISTORY"   text="_addressLookupStartNewSearch" keyStroke="F2"/>
      <Action dataKey="REFINE_WITHOUT_ADDRESS_LIST"  text="_refineAddress" keyStroke="F8" />
