<?xml version="1.0" encoding="UTF-8"?>
<FieldLayoutSet xmlns="http://micros.com/xstore/config/form/fieldlayout" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FieldLayoutConfig.xsd">
  <FieldLayout name="addressLookupLayout" type="ListLayout">
    <Row>
      <Column start="0" width="100" fieldRef="addressLineLabel" visibilityGroup="avsAddressLineVisibility"/>
    </Row>
    <Row>
      <Column start="0" width="100" fieldRef="addressLine" visibilityGroup="avsAddressLineVisibility"/>
    </Row>
    <Row>
      <Column start="0" width="100" fieldRef="addressGroupLabel" visibilityGroup="avsAddressListVisibility"/>
    </Row>
    <Row fill="true">
      <Column start="0" width="100" fieldRef="addressGroups" visibilityGroup="avsAddressListVisibility"/>
    </Row>
    <Row>
      <Column start="0" width="100" fieldRef="selectedAddressLabel" visibilityGroup="avsSelectedAddressVisibility"/>
    </Row>
  </FieldLayout> 
</FieldLayoutSet>