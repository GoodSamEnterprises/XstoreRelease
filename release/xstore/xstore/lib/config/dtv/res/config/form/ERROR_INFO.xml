<FormViewSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormsConfig.xsd">
  <FormView>
    <FormKey dtype="String">ERROR_INFO</FormKey>
    <FormLocation dtype="String">POPUP_VIEW_PANEL</FormLocation>
    <FormLayout dtype="String">LEFT_TO_RIGHT</FormLayout>
    <FormViewPanel>
      <PanelType dtype="String">DETAIL</PanelType>
      <FormTabKey dtype="String">DEFAULT</FormTabKey>
      <Layout dtype="Class">dtv.ui.layout.TableLayout</Layout>
      <LayoutParameters dtype="ParameterList">
        <Parameter name="setColumn">
          <!--MINIMUM, FILL-->
          <param_value dtype="DoubleArray">-3.0, -1.0</param_value>
        </Parameter>
        <Parameter name="setRow">
          <!-- PREFERRED, FILL, PREFERRED, FILL -->
          <param_value dtype="DoubleArray">-2.0, -1.0, -2.0, -1.0</param_value>
        </Parameter>
        <Parameter name="setGaps">
          <param_value dtype="IntegerArray">0, 2, 0, 2</param_value>
        </Parameter>
        <Parameter name="setVerticalStretch">
          <param_value dtype="Boolean">true</param_value>
        </Parameter>
      </LayoutParameters>
      <FormViewCell>
        <LayoutLocation dtype="String">0, 0, 1, 1, R, F</LayoutLocation>
        <Type dtype="String">FormLabel</Type>
        <TextKey dtype="Translatable">_errorInfoName</TextKey>
        <Font fontSize="+10" />
      </FormViewCell>
      <FormViewCell>
        <LayoutLocation dtype="String">1, 0, 1, 1, F, F</LayoutLocation>
        <Type dtype="String">FormTextField</Type>
        <Resource dtype="String">userName</Resource>
        <DataField ref="NameFull" />
        <Required dtype="Boolean">true</Required>
        <Font fontSize="+10" />
      </FormViewCell>
      <FormViewCell>
        <LayoutLocation dtype="String">0, 1, 1, 1, R, F</LayoutLocation>
        <Type dtype="String">FormLabel</Type>
        <TextKey dtype="Translatable">_errorInfoDescription</TextKey>
        <Font fontSize="+10" />
      </FormViewCell>
      <FormViewCell>
        <LayoutLocation dtype="String">1, 1, 1, 1, F, F</LayoutLocation>
        <Type dtype="String">FormLongTextField</Type>
        <Resource dtype="String">errorDescription</Resource>
        <Required dtype="Boolean">true</Required>
        <Font fontSize="+4" />
      </FormViewCell>
      <FormViewCell>
        <LayoutLocation dtype="String">0, 2, 1, 1, R, F</LayoutLocation>
        <Type dtype="String">FormLabel</Type>
        <TextKey dtype="Translatable">_errorInfoModule</TextKey>
        <Font fontSize="+10" />
      </FormViewCell>
      <FormViewCell>
        <LayoutLocation dtype="String">1, 2, 1, 1, F, F</LayoutLocation>
        <Type dtype="String">FormComboBox</Type>
        <Resource dtype="String">errorModule</Resource>
        <Font fontSize="+10" />
      </FormViewCell>
    </FormViewPanel>
    <!--**********************************-->
    <!--** DEFAULT action keys **-->
    <!--**********************************-->
    <ActionGroup key="DEFAULT">
      <Action ref="CANCEL_BACK" />
      <Action ref="ACCEPT" />
    </ActionGroup>
  </FormView>
</FormViewSet>
