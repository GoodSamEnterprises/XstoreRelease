<?xml version="1.0" encoding="UTF-8"?>
<PriceRuleGroup dtype="PriceRuleGroup" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="PriceRuleConfig.xsd">
  <!--
    - The regular price rule should always be here, otherwise the item
    - will never have a price.
  -->
  <PriceRule dtype="PriceRule">
    <Name dtype="String">RegularPrice</Name>
    <Type dtype="String">BasePrice</Type>
    <PriceCode dtype="String">REGULAR_PRICE</PriceCode>
  </PriceRule>
</PriceRuleGroup>
