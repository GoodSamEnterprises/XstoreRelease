        <Parameter name="setVerticalStretch">
          <param_value dtype="Boolean">false</param_value>
        </Parameter>
      </LayoutParameters>
      <FormViewCell>
        <LayoutLocation dtype="String">0, 0, 3, 1, L, C</LayoutLocation>
        <Type dtype="String">FormLabel</Type>
        <Resource dtype="String">message</Resource>
        <Font fontStyle="BOLD" fontSize="+3" />
      </FormViewCell>
      <FormViewCell>
        <LayoutLocation dtype="String">1, 2, 1, 1, F, F</LayoutLocation>
        <Type dtype="String">dtv.pos.reporting.ui.SelectReportParamPanel</Type>
        <Resource dtype="String">params</Resource>
      </FormViewCell>
    </FormViewPanel>
