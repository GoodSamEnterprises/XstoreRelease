<?xml version="1.0" encoding="UTF-8"?>
  <!-- To add a button that contains multiple characters use this syntax:
  <button keyStroke=".COM" composite="true" />
  -->
  <Keyboard name="Virtual Keyboard" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="KeyboardConfig.xsd">
     <layout name="qwerty">
      <panel name="Auxiliary Panel">
        <row>
          <button keyStroke="." />
          <button keyStroke="," />
          <button keyStroke="-" />
          <button keyStroke="+" />
        </row>
        <row>
          <button keyStroke="@" />
          <button keyStroke="!" />
          <button keyStroke="?" />
          <button keyStroke="_" />
        </row>
        <row>
          <button background="_imgEnvironmentDefault" backgroundPressed="_imgEnvironmentPressed" keyAction="showXenvironmentWorkers" horizontalSpan="true"/>
        </row>
      </panel>
      <panel name="Typewriter Panel" alignment="center" modifiable="true">
        <row>
          <button keyStroke="q" />
          <button keyStroke="w" />
          <button keyStroke="e" />
          <button keyStroke="r" />
          <button keyStroke="t" />
          <button keyStroke="y" />
          <button keyStroke="u" />
          <button keyStroke="i" />
          <button keyStroke="o" />
          <button keyStroke="p" />
        </row>
        <row>
          <button keyStroke="a" />
          <button keyStroke="s" />
          <button keyStroke="d" />
          <button keyStroke="f" />
          <button keyStroke="g" />
          <button keyStroke="h" />
          <button keyStroke="j" />
          <button keyStroke="k" />
          <button keyStroke="l" />
        </row>
        <row>
          <button background="_imgTypeShiftDefault" modifier="true" backgroundPressed="_imgTypeShiftPressed" keyStroke="SHIFT" />
          <button keyStroke="z" />
          <button keyStroke="x" />
          <button keyStroke="c" />
          <button keyStroke="v" />
          <button keyStroke="b" />
          <button keyStroke="n" />
          <button keyStroke="m" />
          <button background="_imgTypeLeftDefault" backgroundPressed="_imgTypeLeftPressed" keyStroke="Left" />
          <button background="_imgTypeRightDefault" backgroundPressed="_imgTypeRightPressed" keyStroke="Right" />
        </row>
        <row>
          <button background="_imgTypeBackspaceDefault" backgroundPressed="_imgTypeBackspacePressed" keyStroke="BACK_SPACE" width="1.4" />
          <button text="_typeSpace" keyStroke="Space" horizontalSpan="true" />
          <button background="_imgTypeEnterDefault" backgroundPressed="_imgTypeEnterPressed" keyStroke="Enter" width="1.4" />
        </row>
      </panel>
      <panel name="Numeric Keypad">
        <row>
          <button keyStroke="7" />
          <button keyStroke="8" />
          <button keyStroke="9" />
          <button keyStroke="+" />
        </row>
        <row>
          <button keyStroke="4" />
          <button keyStroke="5" />
          <button keyStroke="6" />
          <button keyStroke="-" />
        </row>
        <row>
          <button keyStroke="1" />
          <button keyStroke="2" />
          <button keyStroke="3" />
          <button background="_imgNumEnterDefault" backgroundPressed="_imgNumEnterPressed" keyStroke="Enter" height="2r"/>
        </row>
        <row>
          <button keyStroke="0" />
          <button keyStroke="." />
          <button background="_imgNumBackspaceDefault" backgroundPressed="_imgNumBackspacePressed" keyStroke="BACK_SPACE" />
        </row>
      </panel>
    </layout>
  </Keyboard>