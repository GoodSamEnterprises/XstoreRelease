  <bean id="defaultPrinterType" class="dtv.pos.framework.reporting.PrinterType">
    <!--
      The entry key can be nearly anything from
      javax.print.attribute.standard, or the fully qualified class of something
      that implements javax.print.attribute.PrintServiceAttribute.
      (See "All Known Implementing Classes" at
       http://docs.oracle.com/javase/7/docs/api/javax/print/attribute/PrintServiceAttribute.html
       for appropriate values.)
      The "value" is either the name of an enumerated instance, or is passed to the
      constructor, depending on the "syntax" of the attribute.
    -->
    <!--
    <entry key="PrinterName" value="HP LaserJet 6MP PS" />
      OR
    <entry key="javax.print.attribute.standard.PrinterName" value="HP LaserJet 6MP PS" />
    -->
    <!--
    <entry key="PrinterLocation" value="Copy Room" />
      OR
    <entry key="javax.print.attribute.standard.PrinterLocation" value="Copy Room" />
    -->
    <!--<property name="attributes">
      <map>
        <entry key="" value="" />
      </map>
    </property>-->
  </bean>
  <bean id="defaultPrintRequestType" class="dtv.pos.framework.reporting.PrintRequestType">
    <!--
      Report configs have the option of specifying a PrintRequestType (or default to 'defaultPrintRequestType')
      A PrintRequestType  specifies parameters where the *name* is the fully qualified class
      name of an implementation of javax.print.attribute.PrintRequestAttribute.
      (See "All Known Implementing Classes" at
       http://docs.oracle.com/javase/7/docs/api/javax/print/attribute/PrintRequestAttribute.html
       for appropriate values.)
      The following entry is given as an example.  In most cases, it is not even necessary
      to specify paper size like this as the Xstore reporting framework will attempt to match
      the report size with the printer's default paper size
    -->
    <!--
    <entry key="MediaSizeName" value="ISO_A4" />
      OR
    <entry key="javax.print.attribute.standard.MediaSizeName" value="ISO_A4" />
     -->
    <!--
    <entry key="Copies" value="2" />
      OR
    <entry key="javax.print.attribute.standard.Copies" value="2" />
    -->
    <!--
    <entry key="MediaTray" value="TOP" />
      OR
    <entry key="javax.print.attribute.standard.MediaTray" value="TOP" />
    -->
    <!--<property name="attributes">
      <map>
        <entry key="" value="" />
      </map>
    </property>-->
  </bean>
  <bean id="taxFreePrinterType" class="dtv.pos.framework.reporting.PrinterType" parent="defaultPrinterType">
