    <!--
    <entry key="MediaSizeName" value="ISO_A4" />
      OR
    <entry key="javax.print.attribute.standard.MediaSizeName" value="ISO_A4" />
     -->
    <!--
    <entry key="Copies" value="2" />
      OR
    <entry key="javax.print.attribute.standard.Copies" value="2" />
    -->
    <!--
    <entry key="MediaTray" value="TOP" />
      OR
    <entry key="javax.print.attribute.standard.MediaTray" value="TOP" />
    -->
    <property name="attributes">
      <map>
        <entry key="javax.print.attribute.standard.JobName" value="TaxFree" />
      </map>
    </property>
  </bean>
  <bean id="reportTaxFreePrintRequestType" class="dtv.pos.framework.reporting.PrintRequestType" parent="defaultPrintRequestType">
    <!--
      Report configs have the option of specifying a PrintRequestType (or default to 'defaultPrintRequestType')
      A PrintRequestType  specifies parameters where the *name* is the fully qualified class
      name of an implementation of javax.print.attribute.PrintRequestAttribute.
      (See "All Known Implementing Classes" at
       http://docs.oracle.com/javase/7/docs/api/javax/print/attribute/PrintRequestAttribute.html
