<?xml version="1.0" encoding="UTF-8"?>
<!-- never saved in FormDesigner -->
<!-- NOTE: THIS CONFIG SHOULD BE MAINTAINED USING dtv.pos.framework.form.design.FormDesigner ANY OTHER EDITS MAY GET LOST -->
<FormViewSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormsConfig.xsd">
  <FormView>
    <FormKey dtype="String">LABEL_LAYOUT_CALCULATING</FormKey>
    <FormLocation dtype="String">POPUP_VIEW_PANEL</FormLocation>
    <FormViewPanel>
      <PanelType dtype="String">DETAIL</PanelType>
      <FormTabKey dtype="String">DEFAULT</FormTabKey>
      <Layout dtype="Class">dtv.ui.layout.TableLayout</Layout>
      <LayoutParameters dtype="ParameterList">
        <Parameter name="setColumn">
          <!--0.2, FILL, 0.2-->
          <param_value dtype="DoubleArray">0.2, -1.0, 0.2</param_value>
        </Parameter>
        <Parameter name="setRow">
          <!--FILL, 0.05, 0.2-->
          <param_value dtype="DoubleArray">-1.0, 0.05, 0.2</param_value>
        </Parameter>
        <Parameter name="setVerticalStretch">
          <param_value dtype="Boolean">true</param_value>
        </Parameter>
      </LayoutParameters>
      <FormViewCell>
        <LayoutLocation dtype="String">1, 0, 1, 1, C, F</LayoutLocation>
        <Type dtype="String">FormLabelWrapping</Type>
        <TextKey dtype="Translatable">_layingOutLabelsMessage</TextKey>
        <Font fontStyle="BOLD" fontSize="+8" />
      </FormViewCell>
      <FormViewCell>
        <LayoutLocation dtype="String">1, 1, 1, 1, F, F</LayoutLocation>
        <Type dtype="String">FormProgress</Type>
        <Font fontStyle="BOLD" />
      </FormViewCell>
    </FormViewPanel>
    <!--**********************************-->
    <!--** DEFAULT action keys **-->
    <!--**********************************-->
    <ActionGroup key="DEFAULT">
      <Action ref="CANCEL" />
      <Action keyStrokeKey="F1" keyStroke="F1" visible="false" />
    </ActionGroup>
  </FormView>
</FormViewSet>
