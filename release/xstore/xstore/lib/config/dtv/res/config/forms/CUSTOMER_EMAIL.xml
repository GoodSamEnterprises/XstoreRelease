<?xml version="1.0" encoding="UTF-8"?>
<FormSet xmlns="http://micros.com/xstore/config/form" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormConfig.xsd">

  <Form name="CUSTOMER_EMAIL" type="POPUP_VIEW_PANEL">
    <Page name="customerEmail">
      <Panel startX="0" startY="0" width="100" height="100" fieldLayoutRef="customerEmailLayout" verticalAlignment="Center" />
    </Page>
    
    <ActionGroup name="DEFAULT">
      <Action dataKey="EMAIL_CANCEL" text="_emailCancel" keyStroke="Escape" />
      <Action dataKey="EMAIL_ONLY" text="_okEmailOnly" keyStroke="F4" />
      <Action dataKey="EMAIL_AND_PRINT" text="_okEmailAndPrint" keyStroke="F8" />
    </ActionGroup>
  </Form>
  
</FormSet>