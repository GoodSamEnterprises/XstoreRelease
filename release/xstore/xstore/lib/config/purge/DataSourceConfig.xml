<?xml version="1.0" encoding="UTF-8"?>
<DataSourceSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../dtv/res/config/DataSourceConfig.xsd">
  <!-- When purging, disable all query timeouts unless explicitly assigned to a data source.  The 
  programmatically enforced default of 300 seconds is not going to be sufficient when purging large 
  data sets. -->
  <Property key="QueryTimeout" value="0" />
</DataSourceSet>
