<?xml version="1.0" encoding="UTF-8"?>
<!-- Maps data object id's (IObjectId) to their respective Persistence Manager Types (see PersistenceManagerConfig.xml
-->
<PmTypeMappingSet xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="PmTypeMappingConfig.xsd">

  <!--Load property tables required by MX in order to create invoices-->
  <PmTypeMapping ObjectId="dtv.xst.dao.tax.TaxGroupRuleId"                            PmType="REGISTER_CORE" LoadProperties="true"/>
  <PmTypeMapping ObjectId="dtv.xst.dao.tax.TaxGroupRulePropertyId"                    PmType="REGISTER_CORE"/>
  <PmTypeMapping ObjectId="dtv.xst.dao.tnd.TenderId"                                  PmType="REGISTER_CORE" LoadProperties="true"/>
  <PmTypeMapping ObjectId="dtv.xst.dao.tnd.TenderPropertyId"                          PmType="REGISTER_CORE"/>
  <PmTypeMapping ObjectId="dtv.xst.dao.com.CodeValueId"                               PmType="CODES" LoadProperties="true"/>
  <PmTypeMapping ObjectId="dtv.xst.dao.com.CodeValuePropertyId"                       PmType="CODES"/>

</PmTypeMappingSet>
