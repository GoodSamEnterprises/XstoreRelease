<?xml version="1.0" encoding="UTF-8"?>
<DataFields xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="DataFieldConfig.xsd">

  <!-- Note: the "editFormat" definitions for the data fields in this file can be found in the
        FormatResource properties bundle (e.g., FormatResource_en.properties). -->

  <DataField name="Phone" editFormat="Numeric">
    <Parameter name="maximumIntegerDigits">
      <param_value dtype="Integer"><![CDATA[10]]></param_value>
    </Parameter>
  </DataField>

  <DataField name="CFDIConfirmationNumber" editFormat="CaseAllUpper">
    <Parameter name="maxCharacters">
      <param_value dtype="Integer"><![CDATA[5]]></param_value>
    </Parameter>
  </DataField>

</DataFields>
