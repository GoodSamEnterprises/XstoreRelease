<?xml version="1.0" encoding="UTF-8"?>
<FieldSet xmlns="http://micros.com/xstore/config/form/field" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormFieldConfig.xsd">

  <!-- Certification informatiom -->
  <Field name="frCertificationLogo" type="Image" text="_countrypack.invoice.nfCertificationLogo" />

  <Field name="frCertNumberLabel" type="Label" text="_countrypack.fr.certNumber" />
  <Field name="frCertNumberValue" type="Label" resource="certificationNumber" />
  <Field name="frCertSoftwareLabel" type="Label" text="_countrypack.fr.certSwVersion" />
  <Field name="frCertSoftwareValue" type="Label" resource="certifiedSoftwareVersion" />
  <Field name="frCurSoftwareLabel" type="Label" text="_countrypack.fr.currentSwVersion" />
  <Field name="frCurSoftwareValue" type="Label" resource="currentSoftwareVersion" />

  <!-- Tampering check  -->
  <Field name="frTamperingCheckRegisterLabel" type="Label" text="_formTranSearchRgstNbr" />
  <Field name="frTamperingCheckRegister" type="Text" resource="registerId" dataFieldRef="Numeric" />
  <Field name="frTamperingCheckSequenceLabel" type="Label" text="_countrypack.fr.formDocumentType" />
  <Field name="frTamperingCheckSequence" type="ComboBox" resource="docType" />


</FieldSet>