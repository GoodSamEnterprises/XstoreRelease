      <element name="INVOICE_FR_SIRET" value="RetailLocation.getStringProperty(FR_SIRET)"/>
      <element name="INVOICE_FR_TVA" value="RetailLocation.getStringProperty(FR_TVA)"/>
      <element name="INVOICE_FR_NAF" value="RetailLocation.getStringProperty(FR_NAF_CODE)"/>
      <element name="INVOICE_SIGNATURE_SHORT" value="StringProperty(CIVC_SIGNATURE_SHORT)"/>
