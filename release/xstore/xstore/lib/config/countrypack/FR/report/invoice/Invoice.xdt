    <label name="FR_SIRET_LABEL" value="_countrypack.fr.siret" />
    <label name="FR_TVA_LABEL" value="_countrypack.fr.tva" />
    <label name="FR_NAF_LABEL" value="_countrypack.fr.nafCode" />
