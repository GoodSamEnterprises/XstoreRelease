<?xml version="1.0" encoding="UTF-8"?>
<FieldLayoutSet xmlns="http://micros.com/xstore/config/form/fieldlayout" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FieldLayoutConfig.xsd">

  <FieldLayout name="customerAddressInformationLayout" type="ListLayout">
    <Row>
      <Column start="0" width="15" fieldRef="organizationNameLabel" horizontalAlignment="Right" />
      <Column start="15" width="49" fieldRef="organizationName" refreshOnChange="true" fieldSubmitPriority="2" />
      <Column start="64" width="12" fieldRef="organizationTypeCodeLabel" horizontalAlignment="Right" />
      <Column start="76" width="24" fieldRef="organizationTypeCode" />
    </Row>
    <Row>
      <Column start="0" width="15" fieldRef="address1Label" horizontalAlignment="Right" />
      <Column start="15" width="49" fieldRef="address1" required="true" />
      <Column start="64" width="12" fieldRef="apartmentLabel" visibilityGroup="apartmentVisibility"
              horizontalAlignment="Right" />
      <Column start="76" width="24" fieldRef="apartment" visibilityGroup="apartmentVisibility" />
    </Row>
    <Row>
      <Column start="15" width="49" fieldRef="address2" />
      <Column start="64" width="12" fieldRef="countryLabel" horizontalAlignment="Right" />
      <Column start="76" width="24" fieldRef="country" />
    </Row>
    <Row>
      <Column start="0" width="15" fieldRef="postalCodeLabel" horizontalAlignment="Right" />
      <Column start="15" width="13" fieldRef="postalCode" refreshOnChange="true" required="true"/>
      <Column start="28" width="13" fieldRef="cityLabel" horizontalAlignment="Right" />
      <Column start="41" width="23" fieldRef="city" required="true" />
      <Column start="64" width="12" fieldRef="countyLabel" horizontalAlignment="Right" />
      <Column start="76" width="24" fieldRef="stateCustMaint"/>
    </Row>
    <Row>
      <Column start="0" width="15" fieldRef="countyLabel" visibilityGroup="countyVisibility"
              horizontalAlignment="Right" />
      <Column start="15" width="20" fieldRef="county" visibilityGroup="countyVisibility" />
      <Column start="35" width="15" fieldRef="neighborhoodLabel" visibilityGroup="neighborhoodVisibility"
              horizontalAlignment="Right" />
      <Column start="50" width="20" fieldRef="neighborhood" visibilityGroup="neighborhoodVisibility" />
    </Row>
  </FieldLayout>

  <FieldLayout name="franceCertificationLogo" type="ListLayout">
    <Row fill="true">
      <Column start="0" width="100" fieldRef="frCertificationLogo" verticalAlignment="Center" horizontalAlignment="Center" />
    </Row>
  </FieldLayout>

  <FieldLayout name="franceCertificationInfo" type="ListLayout">
    <Row>
      <Column start="0"  width="50" fieldRef="frCertNumberLabel" horizontalAlignment="Right" />
      <Column start="50"  width="50" fieldRef="frCertNumberValue" />
    </Row>
    <Row>
      <Column start="0"  width="50" fieldRef="frCertSoftwareLabel" horizontalAlignment="Right" />
      <Column start="50"  width="50" fieldRef="frCertSoftwareValue" />
    </Row>
    <Row>
      <Column start="0"  width="50" fieldRef="frCurSoftwareLabel" horizontalAlignment="Right" />
      <Column start="50"  width="50" fieldRef="frCurSoftwareValue" />
    </Row>
  </FieldLayout>

  <FieldLayout name="franceDBTamperingCheck" type="ListLayout">
    <Row>
      <Column start="0"  width="30" fieldRef="frTamperingCheckRegisterLabel" horizontalAlignment="Right" />
      <Column start="32"  width="68" fieldRef="frTamperingCheckRegister" />
    </Row>
    <Row>
      <Column start="0"  width="30" fieldRef="commonStartDateLabel" horizontalAlignment="Right" />
      <Column start="32"  width="68" fieldRef="commonStartDate" />
    </Row>
    <Row>
      <Column start="0"  width="30" fieldRef="commonEndDateLabel" horizontalAlignment="Right" />
      <Column start="32"  width="68" fieldRef="commonEndDate" />
    </Row>
    <Row>
      <Column start="0"  width="30" fieldRef="frTamperingCheckSequenceLabel" horizontalAlignment="Right" />
      <Column start="32"  width="68" fieldRef="frTamperingCheckSequence" />
    </Row>
  </FieldLayout>

</FieldLayoutSet>