<Header target_org_node="OPENLOCATIONS" deployment_name="DISCOUNT_LIMITS" download_time="IMMEDIATE" apply_immediately="true" />
RUN_SQL|UPDATE SEC_PRIVILEGE SET GROUP_MEMBERSHIP='mwI=', SECOND_PROMPT_GROUP_MEMBERSHIP='mwI=' WHERE PRIVILEGE_TYPE='EJOURNAL_REPRINT_RECEIPT'
RUN_SQL|update dsc_discount set MAX_PERCENTAGE=0.05, PRIVILEGE_TYPE='MODIFY_DISCOUNT' where DISCOUNT_CODE like 'TRANS_PCT_PROMPT' or DISCOUNT_CODE like 'ITEM_PCT_PROMPT' or DISCOUNT_CODE like 'GRP_PCT_PROMPT' 
RUN_SQL|update dsc_discount set MAX_AMOUNT=10, PRIVILEGE_TYPE='MODIFY_DISCOUNT' where DISCOUNT_CODE like 'TRANS_AMT_PROMPT' or DISCOUNT_CODE like 'ITEM_AMT_PROMPT' or DISCOUNT_CODE like 'GRP_AMT_PROMPT'
RUN_SQL|update sec_privilege set overridable_flag=1 where privilege_type='MODIFY_DISCOUNT'
RUN_SQL|update sec_privilege set overridable_flag=1 where privilege_type='DISCOUNT_EXCEED_MAX_THRESHOLD'
RUN_SQL|Update SEC_PRIVILEGE set GROUP_MEMBERSHIP='ngI=', SECOND_PROMPT_GROUP_MEMBERSHIP='ngI=' WHERE ORGANIZATION_ID = '1000' AND PRIVILEGE_TYPE IN ('DISCOUNT_EXCEED_MAX_THRESHOLD') 