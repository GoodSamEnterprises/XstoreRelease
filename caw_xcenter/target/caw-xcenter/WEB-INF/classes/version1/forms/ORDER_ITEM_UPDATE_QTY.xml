<FormSet xmlns="http://micros.com/xstore/config/form" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="FormConfig.xsd">

  <Form name="ORDER_ITEM_UPDATE_QTY" type="POPUP_VIEW_PANEL">
    <Page name="orderItemUpdateQtyPage">
      <Panel startX="0" startY="0" width="100" height="100" fieldLayoutRef="orderItemUpdateQtyLayout" verticalAlignment="Center" />
    </Page>
    
    <ActionGroup name="DEFAULT">
      <Action name="CANCEL" dataKey="EXIT" text="_cancel" keyStroke="Escape" />
      <Action name="ACCEPT" dataKey="ACCEPT" text="_process" keyStroke="F8" />
    </ActionGroup>
  </Form>
</FormSet>